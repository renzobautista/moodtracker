
<div class="container">
  <div class="container">

    <br><br><br>

    <ul class="collection with-header">
      <li class="collection-header"><h3>Community Logs</h3></li>
    <% for log in @logs %>
      <li class="collection-item">
        <div>
          <p><%= log.summary %></p>
          <p style="text-align:right;margin-bottom:0"><%= log.date.strftime("%b %e") %></p>
        </div>
      </li>
    <% end %>
    </ul>
  </div>
</div>